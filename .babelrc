{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"browsers": ["last 2 versions", "ie 10"]
				}
			}
		]
	],
	"retainLines": true,
	"plugins": [
		[
			"@babel/plugin-proposal-object-rest-spread",
			{
				"useBuiltIns": true
			}
		],
		[
			"module-resolver",
			{
				"root": ["./src/media/js"],
				"alias": {
					"classes": "./src/media/js/classes",
					"helpers": "./src/media/js/helpers",
					"modules": "./src/media/js/modules",
					"utils": "./src/media/js/utils"
				}
			}
		]
	]
}
